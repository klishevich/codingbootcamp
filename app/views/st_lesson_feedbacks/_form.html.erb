<%= simple_form_for(@feedback, url: st_my_course_st_my_lesson_st_lesson_feedbacks_path, :method => :post) do |f| %>
  <div class="form-inputs">
    <%= f.input :grade, collection: StLessonFeedback.grades.collect{|item, val| [StLessonFeedback.human_enum_name(:grade, item), item]} %>
    <%= f.input :grade_comment %>
    <%= f.input :hw_done %>
    <%= f.input :hw_link %>
  </div>
  <div class="form-actions">
    <%= f.button :submit, "Сохранить", class: 'btn btn-primary btn-lg' %>
  </div>
<% end %>
